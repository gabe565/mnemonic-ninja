{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>gabe565/renovate-config",
    "local>gabe565/renovate-config:golang"
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.go$"],
      "matchStrings": [
        "\\/\\/ ?renovate branch=(?<currentValue>.+)\\n.*https:\\/\\/github\\.com\\/(?<depName>.+)\\/raw\\/(?<currentDigest>[^\\/]+)"
      ],
      "datasourceTemplate": "git-refs",
      "packageNameTemplate": "https://github.com/{{depName}}"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["cmusphinx/cmudict"],
      "automerge": true
    }
  ]
}
